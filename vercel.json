{
    
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/sitemap.xml", "dest": "/src/pages/SitemapPage.jsx" },
    { "src": "/robots.txt", "dest": "/src/pages/RobotsPage.jsx" }
  ], 
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]

}
